{

  "description": "",
  "manifest_version": 2,
  "name": "Resume Watching",
  "version": "1.4",
  "homepage_url": "https://github.com/valorcurse/keepwatching",
  "icons": {
    "128": "icons/icon-128.png"
  },
  "permissions": [
    "storage",
    "tabs"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "addon@resumewatching.dev"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["keepwatching.js"]
    }
  ]

}
